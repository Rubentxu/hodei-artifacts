{
  "name": "hodei-artifacts-e2e",
  "version": "1.0.0",
  "description": "End-to-end tests for Hodei Artifacts using Playwright",
  "type": "module",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:ui": "playwright test --ui",
    "report": "playwright show-report",
    "install": "playwright install"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "24.3.0",
    "typescript": "^5.0.0"
  },
  "keywords": ["e2e", "playwright", "testing", "hodei", "artifacts"],
  "author": "Hodei Team",
  "license": "MIT"
}
