    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "msw": "^2.10.5",
    "openapi-typescript": "^7.9.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "puppeteer": "^24.19.0",
    "tailwindcss": "^4.1.12",
    "typescript": "~5.8.3",
